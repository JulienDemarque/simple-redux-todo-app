{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A simple boilerplate for react project",
  "main": "server.js",
  "scripts": {
    "format": "prettier --write \"js/**/*.{js,jsx}\"",
    "build": "webpack",
    "watch": "webpack --watch",
    "dev": "webpack-dev-server",
    "lint": "./node_modules/.bin/eslint **/*.{js,jsx} --quiet; exit 0"
  },
  "//": "Comment: format script is useful only if the IDE didn't already integrate prettier...",
  "author": "Julien Demarque",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "webpack": "2.6.1"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-react": "6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-prettier": "2.1.1",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flow": "2.29.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "2.1.1",
    "eslint-plugin-react": "7.0.1",
    "prettier": "^1.3.1",
    "webpack-dev-server": "2.4.5",
    "webpack-cli": "^2.0.9"
  }
}
